;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.ar.org. admin.ar.org. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
    IN      NS      ns1.ar.org.

; PTR Records
100.1   IN      PTR    ns1.ar.org.
131.4   IN      PTR    h1.ar.org.
106.2   IN      PTR    h6.ar.org.
107.2   IN      PTR    h7.ar.org.
